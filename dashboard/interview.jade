.ncg-label DISPLAY
.ncg-btn-group
    .btn-group.btn-group-justified
        .btn-group
            button.btn.btn-xs.btn-primary#sgdq15-interviewUpdateAll Update all names
    .btn-group.btn-group-justified
        .btn-group
            button.btn.btn-xs.btn-success#sgdq15-interviewShow Show
        .btn-group
            button.btn.btn-xs.btn-danger#sgdq15-interviewHide Hide
