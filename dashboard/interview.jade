.ncg-label(style="margin-top: 0") NAMES
.input-group
    input#sgdq15-interviewName1.form-control(type="text" placeholder="First name")
    span.input-group-btn
        button#sgdq15-interviewCancel1.btn.btn-danger
            i.fa.fa-times
        button#sgdq15-interviewSubmit1.btn.btn-success
